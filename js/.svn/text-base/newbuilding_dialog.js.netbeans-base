//Document.Ready - Start of jquery execution
$(document).ready(function(){
    
    //fw_ini_check_div
    if($("#NewBuilding_ajax_validation").val()==0 || $("#NewBuilding_fw_ini").val()=='' ){
        $("div#fw_ini_check_div").hide();
    }
    if($("#NewBuilding_ajax_validation").val()==0 || $("#NewBuilding_date_obs").val()=='' ){
        $("div#fw_date_check_div").hide();
    }
    if($("#NewBuilding_ajax_validation").val()==0 || $("#NewBuilding_page").val()=='' ){
        $("div#fw_page_check_div").hide();
    }
   
     
});