    //fw_ini_check_div
    if($("#Change_ajax_validation").val()==0 || $("#Change_fw_ini").val()=='' ){
        $("div#fw_ini_check_div").hide();
    }
    if($("#Change_ajax_validation").val()==0 || $("#Change_date_obs").val()=='' ){
        $("div#fw_date_check_div").hide();
